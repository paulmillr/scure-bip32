    "mnemonic",
    "phrase",
    "code"
  ],
  "homepage": "https://paulmillr.com/noble/#scure",
  "funding": "https://paulmillr.com/funding/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paulmillr/scure-bip32.git"
  },
  "type": "module",
  "main": "index.js",
